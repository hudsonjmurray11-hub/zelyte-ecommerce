# Shopify Storefront API Configuration
# Copy this file to .env and fill in your actual values

# Your Shopify store domain (without .myshopify.com)
REACT_APP_SHOPIFY_DOMAIN=wf71ya-p7

# Storefront API access token (generate this in your Shopify admin)
REACT_APP_SHOPIFY_STOREFRONT_TOKEN=your_storefront_access_token_here

# API version (optional, defaults to 2024-01)
REACT_APP_SHOPIFY_API_VERSION=2024-01
